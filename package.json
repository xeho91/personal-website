{
  "name": "personal-website",
  "description": "xeho91's personal website project.",
  "keywords": [
    "personal",
    "website"
  ],
  "version": "0.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "author": "xeho91 <xeho91@pm.me> (https://xeho91.site/)",
  "homepage": "https://xeho91.site/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeho91/personal-wesbite.git"
  },
  "bugs": {
    "url": "https://github.com/xeho91/personal-wesbite/issues"
  },
  "scripts": {
    "build": "routify --single-build && vite build",
    "check": "prettier --check --plugin-search-dir \".\"",
    "commit": "gacp --no-add",
    "copy-assets": "run-p copy:*",
    "copy:favicon": "node \"./scripts/copyAssets.js\" favicon",
    "copy:logo": "node \"./scripts/copyAssets.js\" logo",
    "copy:fonts": "node \"./scripts/copyAssets.js\" fonts",
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite",
    "fix": "prettier --write --plugin-search-dir \".\"",
    "fix:css": "run-s \"lint:css -- --fix ./**/*.css\" \"fix ./**/*.css\"",
    "fix:js": "run-s \"lint:js -- --fix ./**/*.js\" \"fix ./**/*.js\"",
    "fix:md": "run-s \"lint:md -- --fix ./**/*.md\" \"fix ./**/*.md\"",
    "fix:svelte": "run-s \"lint:svelte -- --fix ./**/*.md\" \"fix ./**/*.svelte\"",
    "lint:commits": "commitlint --from \"develop\" --to \"HEAD\"",
    "lint:css": "stylelint",
    "lint:ls": "ls-lint",
    "lint:js": "eslint --ext \".js\"",
    "lint:md": "markdownlint",
    "lint:svelte": "eslint --ext \".svelte\" && stylelint",
    "lint:staged": "lint-staged --verbose",
    "start": "vite preview",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.1",
    "@ls-lint/ls-lint": "^1.9.2",
    "@roxi/routify": "^2.15.1",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.6",
    "autoprefixer": "^10.2.5",
    "commitlint": "^12.1.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "fs-extra": "^9.1.0",
    "gacp": "^2.10.2",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "markdownlint": "^0.23.1",
    "markdownlint-cli": "^0.27.1",
    "mdsvex": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-slug": "^6.0.0",
    "stylelint": "^13.12.0",
    "stylelint-8-point-grid": "^1.0.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-declaration-use-variable": "^1.7.2",
    "svelte": "^3.37.0",
    "svelte-preprocess": "^4.7.0",
    "vite": "^2.1.5",
    "vite-aliases": "^0.5.1"
  },
  "dependencies": {
    "@iconify-icons/carbon": "^1.1.3",
    "@iconify-icons/simple-icons": "^1.1.19",
    "@iconify-icons/twemoji": "^1.1.1",
    "@iconify/svelte": "^1.0.4",
    "@xeho91/avatar": "github:xeho91/avatar#main",
    "@xeho91/colors": "github:xeho91/colors#main",
    "@xeho91/fonts": "github:xeho91/fonts#main",
    "@xeho91/logo": "github:xeho91/logo#main",
    "normalize.css": "^8.0.1",
    "svelte-inline-svg": "^1.0.8"
  }
}
