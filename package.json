    "@iconify-icons/twemoji": "^1.1.2",
    "@iconify/svelte": "^1.0.4",
    "@xeho91/assets": "^1.0.0",
    "@xeho91/colors": "^1.2.0",
    "cl-editor": "^2.2.1",
    "normalize.css": "^8.0.1",
