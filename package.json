{
  "$schema": "https://json.schemastore.org/package",
  "name": "personal-website",
  "description": "xeho91's personal website project.",
  "keywords": [
    "personal",
    "website"
  ],
  "version": "0.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "author": "xeho91 <xeho91@pm.me> (https://xeho91.site/)",
  "homepage": "https://xeho91.site/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeho91/personal-wesbite.git"
  },
  "bugs": {
    "url": "https://github.com/xeho91/personal-wesbite/issues"
  },
  "scripts": {
    "build": "svelte-kit build",
    "commit": "gacp --no-add",
    "copy-assets": "run-p copy:*",
    "copy:favicon": "node \"./scripts/copyAssets.js\" favicon",
    "copy:logo": "node \"./scripts/copyAssets.js\" logo",
    "copy:fonts": "node \"./scripts/copyAssets.js\" fonts",
    "dev": "svelte-kit dev",
    "lint:commits": "commitlint --from \"develop\" --to \"HEAD\"",
    "lint:css": "stylelint **/*.css **/*.svelte",
    "lint:ls": "ls-lint",
    "lint:js": "eslint --ext \".js,.cjs,.ts,.svelte\" .",
    "lint:md": "markdownlint **/*.md",
    "lint:svelte": "svelte-check; eslint --ext \".svelte\" .; stylelint **/*.svelte",
    "lint:staged": "lint-staged --verbose",
    "postinstall": "husky install",
    "preview": "svelte-kit preview"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "type": "module",
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.1",
    "@iconify/svelte": "^1.0.4",
    "@ls-lint/ls-lint": "^1.9.2",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "autoprefixer": "^10.2.5",
    "commitlint": "^12.1.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "fs-extra": "^9.1.0",
    "gacp": "^2.10.2",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "markdownlint": "^0.23.1",
    "markdownlint-cli": "^0.27.1",
    "mdsvex": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.14",
    "postcss-cli": "^8.3.1",
    "postcss-mixins": "^7.0.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^7.0.2",
    "postcss-size": "^4.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-slug": "^6.0.0",
    "stylelint": "^13.12.0",
    "stylelint-8-point-grid": "^1.0.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-declaration-use-variable": "^1.7.2",
    "svelte": "^3.38.2",
    "svelte-check": "^1.5.4",
    "svelte-inline-svg": "^1.0.8",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@iconify-icons/carbon": "^1.1.3",
    "@iconify-icons/simple-icons": "^1.1.19",
    "@iconify-icons/twemoji": "^1.1.1",
    "@xeho91/avatar": "github:xeho91/avatar#main",
    "@xeho91/colors": "github:xeho91/colors#main",
    "@xeho91/fonts": "github:xeho91/fonts#main",
    "@xeho91/logo": "github:xeho91/logo#main",
    "normalize.css": "^8.0.1"
  }
}
