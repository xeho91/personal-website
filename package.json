{
  "name": "personal-website",
  "version": "0.1.0",
  "description": "xeho91's personal website project.",
  "keywords": [
    "personal",
    "website"
  ],
  "homepage": "https://xeho91.site/",
  "bugs": {
    "url": "https://github.com/xeho91/personal-wesbite/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "xeho91 <xeho91@pm.me> (https://xeho91.site/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeho91/personal-wesbite.git"
  },
  "scripts": {
    "start": "snowpack --config \"./config/snowpack.config.js\" dev",
    "build": "snowpack --config \"./config/snowpack.config.js\" build",
    "check": "prettier --config \"./config/.prettierrc.js\" --ignore-path \"./config/.prettierignore\" --check",
    "fix": "prettier --config \"./config/.prettierrc.js\" --ignore-path \"./config/.prettierignore\" --write",
    "fix:css": "pnpm lint:css --- --fix \"./**/*.css\" && pnpm fix \"./**/*.css\"",
    "fix:js": "pnpm lint:js --- --fix \"./**/*.js\" && pnpm fix \"./**/*.js\"",
    "fix:md": "pnpm lint:md --- --fix \"./**/*.md\" && pnpm fix \"./**/*.md\"",
    "fix:svelte": "pnpm lint:svelte --- --fix \"./**/*.svelte\" && pnpm fix \"./**/*.svelte\"",
    "lint:commits": "commitlint --config \"./config/.commitlintrc.js\" --from \"develop\" --to \"HEAD\"",
    "lint:css": "stylelint --config \"./config/.stylelintrc.js\" --ignore-path \"./config/.stylelintignore\"",
    "lint:js": "eslint --config \"./config/.eslintrc.js\" --ignore-path \"./config/.eslintignore\" --ext \".js\"",
    "lint:md": "markdownlint --config \"./config/.markdownlint.json\" --ignore-path \"./config/.markdownlintignore\"",
    "lint:svelte": "eslint --config \"./config/.eslintrc.js\" --ignore-path \"./config/.eslintignore\" --ext \".svelte\"",
    "lint:staged": "lint-staged --config \"./config/.lintstagedrc.js\" --verbose",
    "postinstall": "husky install \"config/.husky\""
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@snowpack/plugin-postcss": "^1.1.0",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@snowpack/plugin-svelte": "^3.5.2",
    "commitlint": "^12.0.1",
    "commitlint-config-gitmoji": "^2.2.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "husky": "^5.1.1",
    "lint-staged": "^10.5.4",
    "markdownlint": "^0.23.1",
    "markdownlint-cli": "^0.26.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-nesting": "^7.0.1",
    "postcss-url": "^10.1.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.6",
    "snowpack": "^3.0.13",
    "stylelint": "^13.11.0",
    "stylelint-8-point-grid": "^1.0.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-declaration-use-variable": "^1.7.2",
    "svelte-preprocess": "^4.6.9"
  },
  "dependencies": {
    "@xeho91/avatar": "github:xeho91/avatar#main",
    "@xeho91/colors": "github:xeho91/colors#main",
    "@xeho91/fonts": "github:xeho91/fonts",
    "@xeho91/logo": "github:xeho91/logo",
    "normalize.css": "^8.0.1",
    "svelte": "^3.34.0",
    "svelte-routing": "^1.5.0"
  }
}
